<%- include header%>
<% posts.forEach(function (post) { %>
<p><h2><a href="/user/<%= post.name + '/' + post.title %>/"><%= post.title %></a></h2></p>
<p class="info">
	作者：<a href="/user/<%= post.name %>/"><%= post.name %></a> |
	日期：<%= post.time %>
</p>
<p><%- post.content %></p>
<% }) %>
<%- include pager %>
<%- include footer %>